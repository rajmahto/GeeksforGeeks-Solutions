public:
    vector<int> findTwoElement(vector<int> arr, int n) {
        // code here
        int repeated,missing;
        unordered_map<int,int>mp;
        for(int i=0;i<n;i++){
            mp[arr[i]]++;
        }
        for(int i=1;i<=n;i++){
            if(mp[i] == 2){
                repeated = i;
            }
            else if(mp[i] == 0){
                missing = i;
            }
        }
        return {repeated,missing};
    }
